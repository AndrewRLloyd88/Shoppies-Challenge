-- Name: ingredients_pkey; Type: CONSTRAINT; Schema: public; Owner: andrew; Tablespace: 
--

ALTER TABLE ONLY ingredients
    ADD CONSTRAINT ingredients_pkey PRIMARY KEY (id);


