-- Name: phrases_message_key; Type: CONSTRAINT; Schema: public; Owner: andrew; Tablespace: 
--

ALTER TABLE ONLY phrases
    ADD CONSTRAINT phrases_message_key UNIQUE (message);


--
-- Name: phrases_pkey; Type: CONSTRAINT; Schema: public; Owner: andrew; Tablespace: 
--

ALTER TABLE ONLY phrases
    ADD CONSTRAINT phrases_pkey PRIMARY KEY (id);


--
-- Name: recipes_pkey; Type: CONSTRAINT; Schema: public; Owner: andrew; Tablespace: 
--

ALTER TABLE ONLY recipes
    ADD CONSTRAINT recipes_pkey PRIMARY KEY (id);


