--
-- PostgreSQL database cluster dump
--

SET default_transaction_read_only = off;

SET client_encoding = 'UTF8';
SET standard_conforming_strings = on;

--
-- Roles
--

CREATE ROLE andrew;
ALTER ROLE andrew WITH SUPERUSER INHERIT CREATEROLE CREATEDB LOGIN NOREPLICATION PASSWORD 'md5199b275145ea579da877e9635e87dfa2';
CREATE ROLE postgres;
ALTER ROLE postgres WITH SUPERUSER INHERIT CREATEROLE CREATEDB LOGIN REPLICATION PASSWORD 'md59df270eb52907fff723d9b8b7436113a';






--
-- Database creation
--

CREATE DATABASE andrew WITH TEMPLATE = template0 OWNER = postgres;
CREATE DATABASE ar_exercises WITH TEMPLATE = template0 OWNER = andrew;
CREATE DATABASE codegig WITH TEMPLATE = template0 OWNER = andrew;
CREATE DATABASE forkitall_development WITH TEMPLATE = template0 OWNER = postgres;
CREATE DATABASE forkitall_test WITH TEMPLATE = template0 OWNER = andrew;
CREATE DATABASE jungle_development WITH TEMPLATE = template0 OWNER = postgres;
CREATE DATABASE jungle_test WITH TEMPLATE = template0 OWNER = andrew;
CREATE DATABASE rails_project_development WITH TEMPLATE = template0 OWNER = andrew;
CREATE DATABASE rails_project_test WITH TEMPLATE = template0 OWNER = andrew;
REVOKE ALL ON DATABASE template1 FROM PUBLIC;
REVOKE ALL ON DATABASE template1 FROM postgres;
GRANT ALL ON DATABASE template1 TO postgres;
GRANT CONNECT ON DATABASE template1 TO PUBLIC;


\connect andrew

SET default_transaction_read_only = off;

--
-- PostgreSQL database dump
