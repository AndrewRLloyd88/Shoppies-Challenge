-- Name: index_ingredient_measures_on_recipes_id; Type: INDEX; Schema: public; Owner: andrew; Tablespace: 
--

CREATE INDEX index_ingredient_measures_on_recipes_id ON ingredient_measures USING btree (recipes_id);


--
-- Name: index_users_on_access_token; Type: INDEX; Schema: public; Owner: andrew; Tablespace: 
--

CREATE UNIQUE INDEX index_users_on_access_token ON users USING btree (access_token);


