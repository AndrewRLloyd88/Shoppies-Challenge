--
-- Name: gigs; Type: TABLE; Schema: public; Owner: postgres; Tablespace: 
--

CREATE TABLE gigs (
    id integer NOT NULL,
    title character varying(255),
    technologies character varying(255),
    budget character varying(20),
    description text,
    contact_email character varying(255),
    createdat date,
    updatedat date
);


ALTER TABLE public.gigs OWNER TO postgres;

