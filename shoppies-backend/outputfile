-- Name: followers_pkey; Type: CONSTRAINT; Schema: public; Owner: andrew; Tablespace: 
--

ALTER TABLE ONLY followers
    ADD CONSTRAINT followers_pkey PRIMARY KEY (id);


--
-- Name: ingredient_measures_pkey; Type: CONSTRAINT; Schema: public; Owner: andrew; Tablespace: 
--

ALTER TABLE ONLY ingredient_measures
    ADD CONSTRAINT ingredient_measures_pkey PRIMARY KEY (id);


